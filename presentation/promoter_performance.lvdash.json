    {
      "name": "b24ccc91",
      "displayName": "dim_venue",
      "queryLines": [
        "SELECT * FROM prd_gold.entertainment.dim_venue"
      ]
    },
    {
      "name": "1a378e71",
      "displayName": "promoter_performance",
      "queryLines": [
        "WITH by_venue AS (\r\n",
        "  SELECT\r\n",
        "    p.name AS promoter_name,\r\n",
        "    v.name AS event_name,\r\n",
        "    -- Se existir a coluna real de vendas, troque para: SUM(f.tickets_sold)\r\n",
        "    SUM(f.ticket_limit) AS tickets_sold,\r\n",
        "    COUNT(f.id_event)   AS event_count,\r\n",
        "    MAX(f.ticket_limit) AS max_ticket_limit\r\n",
        "  FROM prd_gold.entertainment.fact_events f\r\n",
        "  JOIN prd_gold.entertainment.dim_venue    v ON f.id_venue    = v.id\r\n",
        "  JOIN prd_gold.entertainment.dim_promoter p ON f.id_promoter = p.id\r\n",
        "  GROUP BY p.name, v.name\r\n",
        ")\r\n",
        "SELECT * FROM (\r\n",
        "SELECT\r\n",
        "  promoter_name,\r\n",
        "  ROUND(AVG(100.0 * max_ticket_limit / NULLIF(tickets_sold, 0)), 2)\r\n",
        "    AS avg_pct_max_ticket_limit_vs_tickets_sold,\r\n",
        "  SUM(tickets_sold) AS total_tickets_sold,\r\n",
        "  SUM(event_count)  AS total_event_count\r\n",
        "FROM by_venue\r\n",
        "WHERE tickets_sold IS NOT NULL\r\n",
        "GROUP BY promoter_name\r\n",
        "ORDER BY\r\n",
        "  avg_pct_max_ticket_limit_vs_tickets_sold DESC,\r\n",
        "  total_tickets_sold DESC,\r\n",
        "  total_event_count ASC\r\n",
        "  ) ORDER BY total_tickets_sold DESC\r\n"
      ]
    },
    {
      "name": "63c44fdd",
      "displayName": "Market Promoter Distribution Analysis",
      "queryLines": [
        "SELECT\r\n",
        "      REPLACE(m.name,'\"',\"\") AS market_name,\r\n",
        "      COUNT(DISTINCT p.id) AS total_promoters,\r\n",
        "      COLLECT_SET(p.name) AS promoters\r\n",
        "FROM prd_gold.entertainment.fact_events e\r\n",
        "JOIN prd_gold.entertainment.dim_market m \r\n",
        "      ON e.id_market = m.id\r\n",
        "JOIN prd_gold.entertainment.dim_promoter p \r\n",
        "      ON e.id_promoter = p.id\r\n",
        "WHERE e.id_promoter != \"\"\r\n",
        "GROUP BY m.name\r\n"
      ]
    },
    {
      "name": "5c8abc99",
      "displayName": "Promoter Events Summary Analysis",
      "queryLines": [
        "SELECT\r\n",
        "      p.name AS promoter_name,\r\n",
        "      COUNT(DISTINCT e.id_event) AS total_events,\r\n",
        "      LOG10(COUNT(DISTINCT e.id_event) + 1) AS log_proportion_total_events\r\n",
        "FROM prd_gold.entertainment.fact_events e\r\n",
        "JOIN prd_gold.entertainment.dim_promoter p \r\n",
        "      ON e.id_promoter = p.id\r\n",
        "WHERE e.id_promoter != \"\"\r\n",
        "GROUP BY p.name\r\n",
        "ORDER BY log_proportion_total_events DESC"
      ]
    }
  ],
